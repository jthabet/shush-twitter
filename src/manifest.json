{
    "name": "Twitter shush",
    "description": "Easily mute words on twitter by selecting them!",
    "version": "0.1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "contextMenus",
        "cookies",
        "downloads"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/tabler-icon-eye-off-black@16.png",
            "32": "icons/tabler-icon-eye-off-black@32.png",
            "48": "icons/tabler-icon-eye-off-black@48.png",
            "128": "icons/tabler-icon-eye-off-black@128.png"
        }
    },
    "icons": {
        "16": "icons/tabler-icon-eye-off-black@16.png",
        "32": "icons/tabler-icon-eye-off-black@32.png",
        "48": "icons/tabler-icon-eye-off-black@48.png",
        "128": "icons/tabler-icon-eye-off-black@128.png"
    },
    "host_permissions": [
        "https://twitter.com/*",
        "https://api.twitter.com/*"
    ],
    "options_ui": {
        "open_in_tab": true,
        "page": "options.html"
    }
}